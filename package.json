{
  "name": "fanz-money-dash",
  "version": "1.0.0",
  "description": "Advanced Financial Management Platform for FANZ Creator Economy",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd src/frontend && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "setup": "./scripts/start.sh",
    "docker:build": "docker build -t fanz-money-dash .",
    "docker:run": "docker run -p 3001:3001 fanz-money-dash"
  },
  "keywords": [
    "fintech",
    "creator-economy", 
    "blockchain",
    "ai-analytics",
    "compliance",
    "pwa",
    "fanz"
  ],
  "author": "FANZ Network",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.2",
    "mongoose": "^7.5.0",
    "redis": "^4.6.8",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "joi": "^17.9.2",
    "uuid": "^9.0.0",
    "web3": "^4.1.1",
    "@tensorflow/tfjs-node": "^4.10.0",
    "stripe": "^13.5.0",
    "axios": "^1.5.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "nodemailer": "^6.9.4",
    "winston": "^3.10.0",
    "express-validator": "^7.0.1",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "cron": "^3.1.3",
    "pdf-lib": "^1.17.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "babel-jest": "^29.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FANZ-Network/FanzMoneyDash"
  },
  "bugs": {
    "url": "https://github.com/FANZ-Network/FanzMoneyDash/issues"
  },
  "homepage": "https://fanz.network"
}