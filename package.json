{
  "name": "fanz-money-dash",
  "version": "1.0.0",
  "description": "Advanced Financial Management Platform for FANZ Creator Economy",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run frontend:dev\"",
    "server:dev": "nodemon src/server.js",
    "frontend:dev": "webpack serve --mode development",
    "build": "npm run build:frontend",
    "build:frontend": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "setup": "./scripts/start.sh",
    "docker:build": "docker build -t fanz-money-dash .",
    "docker:run": "docker run -p 3001:3001 fanz-money-dash"
  },
  "keywords": [
    "fintech",
    "creator-economy",
    "blockchain",
    "ai-analytics",
    "compliance",
    "pwa",
    "fanz"
  ],
  "author": "FANZ Network",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.10.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "cron": "^3.1.3",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.3",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.9",
    "pdf-lib": "^1.17.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.3",
    "redis": "^4.6.8",
    "sharp": "^0.32.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^17.7.0",
    "uuid": "^9.0.0",
    "web3": "^4.1.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "babel-jest": "^29.6.4",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.37.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "style-loader": "^4.0.0",
    "supertest": "^6.3.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FANZ-Network/FanzMoneyDash"
  },
  "bugs": {
    "url": "https://github.com/FANZ-Network/FanzMoneyDash/issues"
  },
  "homepage": "https://fanz.network"
}
