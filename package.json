{
  "name": "fanzmoneydash",
  "version": "1.0.0",
  "description": "Enterprise-grade financial operations system for the FANZ ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "FANZ Network <dev@fanz.network>",
  "license": "PROPRIETARY",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx scripts/migrate.ts",
    "seed": "tsx scripts/seed.ts",
    "create-admin": "tsx scripts/create-admin.ts",
    "generate-keys": "tsx scripts/generate-keys.ts",
    "docker:build": "docker build -t fanzmoneydash .",
    "docker:run": "docker run -p 4000:4000 fanzmoneydash",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/node": "^20.10.4",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "postgres": "^3.4.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "socket.io": "^4.7.5",
    "@tensorflow/tfjs-node": "^4.15.0",
    "sharp": "^0.33.1",
    "jimp": "^0.22.12",
    "ethers": "^6.9.0",
    "web3": "^4.3.0",
    "helia": "^4.0.1",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FANZ/FanzMoneyDash.git"
  },
  "keywords": [
    "fanz",
    "payment-processing",
    "fraud-detection",
    "financial-management",
    "creator-economy",
    "adult-content",
    "fintech"
  ]
}